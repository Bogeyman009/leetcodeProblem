  int maximumWealth(vector<vector<int>>& a) {
        int m=a.size();
        int n=a[0].size();
        
        int mx=0;
       for(int i=0;i<m;i++)
       {    int sum=0;
           for(int j=0;j<n;j++)
           {
               sum+=a[i][j];
           }
           mx=max(sum,mx);
       }
        return mx;
    }
